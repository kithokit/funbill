
<h1>{{title}}</h1>
{{outlet}}
{{#each item in model}}
<div class="wrap wrap-defaults">
	<div class="box-rotate-wrap">
		<div class="box box-rotate box-rotate-top">
			<div class="front">
				<h2>Front</h2>
        {{item.firstName}}
			</div>
			<div class="back">
				<h2>Back</h2>
			</div>
			<div class="top">
				<h2>Top</h2>
        {{item.lastName}}
			</div>
			<div class="bottom">
				<h2>Bottom</h2>
			</div>
			<div class="left">
				<h2>Left</h2>
			</div>
			<div class="right">
				<h2>Right</h2>
			</div>
		</div>
	</div>
</div>
{{/each}}
